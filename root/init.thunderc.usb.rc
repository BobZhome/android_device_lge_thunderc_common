on init
    write /sys/class/android_usb/android0/iSerial $ro.serialno
    write /sys/class/android_usb/android0/f_rndis/manufacturer LGE
    write /sys/class/android_usb/android0/f_rndis/vendorID 1004
    write /sys/class/android_usb/android0/f_rndis/wceis 1
    write /sys/class/usb_composite/rndis/iSerial $ro.serialno
    write /sys/class/usb_composite/rndis/manufacturer LGE
    write /sys/class/usb_composite/rndis/vendorID 1004
    write /sys/class/usb_composite/rndis/wceis 1

on boot
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/iManufacturer $ro.product.manufacturer
    write /sys/class/android_usb/android0/iProduct $ro.product.model
    write /sys/class/android_usb/android0/enable 1
    write /sys/class/usb_composite/rndis/enable 0
    write /sys/class/usb_composite/rndis/iManufacturer $ro.product.manufacturer
    write /sys/class/usb_composite/rndis/iProduct $ro.product.model
    write /sys/class/usb_composite/rndis/enable 1

on property:sys.usb.config=adb
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor 1004
    write /sys/class/android_usb/android0/idProduct 6000
    write /sys/class/android_usb/android0/functions mass_storage,adb
    write /sys/class/android_usb/android0/enable 1
    setprop sys.usb.state $sys.usb.config
    write /sys/class/usb_composite/adb/enable 0
    write /sys/class/usb_composite/adb/idVendor 1004
    write /sys/class/usb_composite/adb/idProduct 6000
    write /sys/class/usb_composite/adb/functions mass_storage,adb
    write /sys/class/usb_composite/adb/enable 1
    setprop sys.usb.state $sys.usb.config

on property:sys.usb.config=mass_storage,adb
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor 1004
    write /sys/class/android_usb/android0/idProduct 61A6
    write /sys/class/android_usb/android0/functions $sys.usb.config
    write /sys/class/android_usb/android0/enable 1
    start adbd
    setprop sys.usb.state $sys.usb.config
    write /sys/class/usb_composite/enable 0
    write /sys/class/usb_composite/idVendor 1004
    write /sys/class/usb_composite/idProduct 61A6
    write /sys/class/usb_composite/functions $sys.usb.config
    write /sys/class/usb_composite/enable 1
    start adbd
    setprop sys.usb.state $sys.usb.config

